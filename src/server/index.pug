mixin asset(name, type = 'js')
  - const assetPath = `/static${assets[name][type]}`
  script(type='text/javascript' src=assetPath)

doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width,initial-scale=1,maximum-scale=1,user-scalable=0')
    title CultureStake
  body
    #app
    +asset('lib')
    +asset('app')
