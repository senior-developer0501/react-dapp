{
  "name": "culturestake",
  "version": "0.1.0",
  "description": "CultureStake is a web-based voting system for cultural decision making and investment",
  "main": "src/server/index.js",
  "scripts": {
    "build": "npm run clear && npm run server:build && npm run client:build",
    "clear": "rimraf ./build",
    "client:build": "cross-env NODE_ENV=production webpack --mode production --progress",
    "client:clear": "rimraf ./build/static",
    "client:watch": "cross-env NODE_ENV=development webpack --mode development --watch --progress",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint './src/client/**/*.js'",
    "lint:js": "eslint --ignore-path .gitignore ./",
    "server:build": "babel ./src/server --out-dir ./build && cp ./src/server/index.pug ./build/index.pug",
    "server:start": "cross-env NODE_ENV=production node ./build/index.js",
    "server:watch": "nodemon --watch ./src/server --exec cross-env NODE_ENV=development babel-node ./src/server/index.js --source-maps inline",
    "start": "npm run server:start",
    "test": "cross-env NODE_ENV=test jest --forceExit --runInBand --detectOpenHandles",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lazaruslabs/culturestake.git"
  },
  "private": true,
  "contributors": [
    "adzialocha",
    "ana0"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/lazaruslabs/culturestake/issues"
  },
  "homepage": "https://github.com/lazaruslabs/culturestake#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "assets-webpack-plugin": "^3.9.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.1.0",
    "jest": "24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^5.5.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^4.0.2",
    "truffle": "^5.1.16",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "celebrate": "^12.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "culturestake-contracts": "^1.0.4",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "http-status": "^1.4.2",
    "immutability-helper": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "luxon": "^1.22.0",
    "metatx-handler": "^1.0.13",
    "method-override": "^3.0.0",
    "mime": "^2.4.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pug": "^2.0.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-form": "^4.0.1",
    "react-redux": "^7.2.0",
